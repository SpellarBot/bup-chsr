@model CHSR.Models.SubSpecialization

@{
    SubSpecialization subSpecialization = (SubSpecialization) ViewData["subSpecialization"];
}

<h1>Edit</h1>

<h4>SubSpecialization</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />

            <div class="form-group">

                Specialization Name : @subSpecialization.Specialization.Name
            </div>

            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <input type="hidden" name="specializationID" value="@subSpecialization.Specialization.Id" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    @*<a asp-action="Index">Back to List</a>*@
</div>

